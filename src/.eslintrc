{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": false
        }
      ],
      "jsx-a11y/no-noninteractive-element-interactions": [
        "off"
      ],
      "react/forbid-prop-types": "off",
      "react/jsx-props-no-spreading": "off"
    }
  }